Toml = Expr*
Expr =
      TableArray
    | Table
    | KeyVal

Table = '[' Key ']' '\n' (KeyVal '\n')*
TableArray = '[[' Key ']]' '\n' KeyVal*
Key = ('str_key' | Key) ('.' ('str_key' | Key))*
KeyVal = Key '=' Value
Array = '[' (Value ( ',' | '\n' ))* ']'
TableInline = '{' KeyVal? (',' KeyVal)* '}'

Value =
      'string'
    | 'number'
    | 'bool'
    | Array
    | TableInline
