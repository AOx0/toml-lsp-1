Toml = Expr*
Expr =
      TableArray
    | Table
    | KeyVal

TableArray = '[[' Key ']]' '\n' KeyVal*
Table = '[' Key ']' '\n' (KeyVal '\n')*

KeyVal = Key '=' Value

Key = KeyPart ('.' KeyPart)*
KeyPart = 'str_key' | 'key'

Value =
      'string'
    | 'number'
    | 'bool'
    | Array
    | TableInline

Array = '[' (Value ( ',' | '\n' ))* ']'
TableInline = '{' KeyVal? (',' KeyVal)* '}'
